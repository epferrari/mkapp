{
	"parser": "babel-eslint",
	"env":{
		"node":true,
		"es6":true,
		"mocha":true
	},
	"rules": {
		"strict":0,
		"eqeqeq":0,
		"quotes":0,
		"comma-spacing":0,
		"no-unused-vars":0,
		"no-ternary":0,
		"no-trailing-spaces": 1,
		"no-multi-spaces": 0,
		"key-spacing": 0,
		"curly": 0,
		"camelcase": 1,
		"no-underscore-dangle":0,
		"new-cap": 0,
		"no-cond-assign": 0,
		"consistent-return":1,
		"no-use-before-define":1,
		"camelcase":0
	},
	"ecmaFeaturesj": {
		"jsx":true,
		"spread":true,
		"generators":true
	}
}
